          { "code": "CUI", "display": "CUI" },
          { "code": "CUIHLTH", "display": "CUI//HLTH" },
          { "code": "CUIHLTHP", "display": "(CUI//HLTH)" },
          { "code": "CUIP", "display": "(CUI)" },
          { "code": "CUIPRVCY", "display": "CUI//PRVCY" },
          { "code": "CUIPRVCYP", "display": "(CUI//PRVCY)" },
          { "code": "CUISP-HLTH", "display": "CUI//SP-HLTH" },
